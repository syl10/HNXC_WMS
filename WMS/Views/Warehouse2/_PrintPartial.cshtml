<script type="text/javascript">
    function printClick() {
        var treeObj = $.fn.zTree.getZTreeObj("warehouseTree");
        var row = treeObj.getSelectedNodes();
        if (row.length>0) {
            var rowAttr = null, rowId = null;
//            alert(row.length);
            if (row[0].type == null) { rowAttr = ""; } else { rowAttr = row[0].type; }
            if (row[0].id == null) { rowId = ""; } else { rowId = row[0].id; }

            window.location.href = "/Warehouse2/CreateExcelToClient/"
                                            + "?type=" + row[0].type 
                                            + "&id=" + row[0].id;
        }
        else {
            $.messager.alert("系统提示", "未选中树形菜单", "info");
        }
    }
</script>
